{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npm run env:check && npx rimraf dist && npx tsc --project tsconfig.node.json",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "test": "vitest run",
    "env:check": "node ../../env-governance/run-check.mjs --schema services/valuer-agent",
    "prestart": "npm run env:check",
    "predev": "npm run env:check"
  },
  "dependencies": {
    "express": "^4.18.2",
    "openai": "^4.28.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "eslint": "^9.9.1",
    "tsx": "^4.7.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.3.0",
    "vitest": "^1.3.1"
  }
}
